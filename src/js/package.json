    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "global-agent": "^3.0.0",
    "globals": "^15.15.0",
    "node-fetch": "^3.3.2",
